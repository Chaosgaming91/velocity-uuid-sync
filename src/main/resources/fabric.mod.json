  },
  "license": "MIT",
  "icon": "assets/velocityuuidforwarder/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "com.chaosgaming.velocityuuidforwarder.VelocityUUIDForwarder"
    ]
  },
  "mixins": [
    "velocityuuidforwarder.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "fabric-api": "*",
    "minecraft": "~1.21.1",
    "java": ">=21"
